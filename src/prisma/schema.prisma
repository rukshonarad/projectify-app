datasource db {
    provider = "mongodb"
    url      = env("DB_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id            String   @id @default(auto()) @map("_id") @db.ObjectId
    email         String   @unique
    firsName      String
    lastName      String
    preferredName String?
    password      String
    createdAt     DateTime @default(now())
    updatedAt     DateTime @updatedAt
}
